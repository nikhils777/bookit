<h1 align="center">Your Products</h1>
<div class="row">
  <div class="col-md-4">

<%= render partial: 'product' %>
  </div>
  <div class="col-md-8">
    <ul>
      <% @products.each do |product| %>
      <li><%= link_to product.name, product_path(product) %></li>
      <% end %>
    </ul>
  </div>
</div>
